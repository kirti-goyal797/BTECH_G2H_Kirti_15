#include <stdio.h>
int main()
{
    char ch ;
    printf("enter a character \n");
    scanf("%c", &ch);
    
    if(ch>= '0' && ch <='9') 
        printf("%c is a digit character \n", ch);
    else if ( ch >= 'a' && ch<= 'z' || ch>= 'A' && ch<= 'Z' )
        printf( " %c is a character \n", ch); 
    else 
            printf("%c is a special character, ch");
}
