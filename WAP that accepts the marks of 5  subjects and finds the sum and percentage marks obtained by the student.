#include<stdio.h>
int main () {
    int a , b, c , d , e , sum;
    float percentage ;
    printf("enter the marks of 5 subjects\n");
    scanf(" %d%d%d%d%d" , &a,&b,&c,&d,&e);
    sum = a + b + c + d + e ;
    percentage = ( sum * 100 / 500);
    printf(" the sum is %d \n" ,sum);
    printf(" the percentage is %f", percentage);
    return 0 ;
}
